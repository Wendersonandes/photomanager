<li class="photo__thumb dim relative " data-id="<%= photo.id %>">
	<%= image_tag eval('photo.file.' + (defined?(photosize) ? photosize : "album") + '.url'), :class => "h-100 dragdrop__handler" %>
	<div class="icon__container absolute bottom-0 pa0 ma0 mb1">
		<i class="fa fa-eye ml1" aria-hidden="true"></i>
		<i class="fa fa-trash ml2 delete__photo" aria-hidden="true"></i>
	</div>
</li>
